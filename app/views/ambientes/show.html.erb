<div class="container">
       <!-- breadcrumbs -->
    <div class="ibox mt-4 col-md-6 mx-auto">
        <div class="ibox-title">
            <h1>Ambientes</h1>
        </div>
        <div class="ibox-content ">
            <h2 class="no-margins ">
                <span class="font-weight-bold">Nombre: </span>
                <%= @ambiente.nombre %>
            </h2>
            <h2 class="no-margins ">
                <span class="font-weight-bold">Descripcion: </span>
                <%= @ambiente.descripcion %>
            </h2>
            <br>
        </div>
    </div>
</div>
